# Docker Environment Variables
# Скопируйте этот файл в .env и заполните своими данными

# Google OAuth Credentials (получите в Google Cloud Console)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Session Secret (сгенерируйте случайную строку)
SESSION_SECRET=your_random_session_secret_minimum_32_characters_long

# Port (по умолчанию 3000)
PORT=3000

# Node Environment
NODE_ENV=production